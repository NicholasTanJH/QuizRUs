CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE PUBLIC.USER(USERID INTEGER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,USERNAME VARCHAR(255) NOT NULL UNIQUE,PASSWORD VARCHAR(255) NOT NULL,EMAIL VARCHAR(255) NOT NULL,FIRSTNAME VARCHAR(255),LASTNAME VARCHAR(255))
CREATE MEMORY TABLE PUBLIC.QUIZ(QUIZID INTEGER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,TITLE VARCHAR(255) NOT NULL,USERID INTEGER NOT NULL,FOREIGN KEY (USERID) REFERENCES PUBLIC.USER(USERID) ON DELETE CASCADE,TIMELIMIT INTEGER)
CREATE MEMORY TABLE PUBLIC.QUESTION(QUESTIONID INTEGER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,QUESTIONTEXT VARCHAR(255) NOT NULL,QUIZID INTEGER NOT NULL,FOREIGN KEY (QUIZID) REFERENCES PUBLIC.QUIZ(QUIZID) ON DELETE CASCADE,QUESTIONTYPE VARCHAR(255) NOT NULL)
CREATE MEMORY TABLE PUBLIC.ANSWER(ANSWERID INTEGER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,ANSWERTEXT VARCHAR(255) NOT NULL,QUESTIONID INTEGER NOT NULL,FOREIGN KEY (QUESTIONID) REFERENCES PUBLIC.QUESTION(QUESTIONID) ON DELETE CASCADE,ISCORRECT BOOLEAN DEFAULT FALSE NOT NULL)
